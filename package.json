{
  "name": "svg-music-logos",
  "description": "A collection of bands' and musicians' logos in SVG.",
  "homepage": "https://tiagoporto.github.io/svg-music-logos/",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Tiago Porto",
    "url": "https://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/svg-music-logos.git"
  },
  "engines": {
    "node": ">= 20",
    "npm": ">= 10"
  },
  "scripts": {
    "prepare": "husky",
    "build": "gulp build",
    "gulp": "gulp watch",
    "checks": "npm run check-formatting && npm run check-docs && npm run lint",
    "check-docs": "remark --frail .",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "check-formatting:fix": "npm run check-formatting -- --write",
    "lint": "eslint --ext js,jsx,ts,tsx . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "update-readme": "gulp update-readme",
    "generate-data": "gulp generate-data"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "gulp": "^4.0.2",
    "gulp-concat-jsons": "git+https://github.com/totango/gulp-concat-jsons.git",
    "gulp-ignore": "^3.0.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-replace": "^1.0.0",
    "gulp-stylus": "^2.7.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "remark-cli": "^12.0.0",
    "remark-emoji": "^4.0.1",
    "remark-gfm": "^4.0.0",
    "remark-lint-no-dead-urls": "^1.1.0",
    "remark-lint-no-url-trailing-slash": "^3.0.1",
    "remark-preset-lint-consistent": "^5.1.2",
    "remark-preset-lint-markdown-style-guide": "^5.1.3",
    "remark-preset-lint-recommended": "^6.1.3",
    "remark-validate-links": "^13.0.0"
  }
}
